#suite insert_test

#test insert_test_1
  char src[BUFFSIZE] = "AAA";
  char str[BUFFSIZE] = "b";
  char* res = s21_insert(src, str, 0);
  ck_assert_pstr_eq("bAAA", res);
  if (res) free(res);

#test insert_test_2
  char src[BUFFSIZE] = "AAA";
  char str[BUFFSIZE] = " ";
  char* res = s21_insert(src, str, 0);
  ck_assert_pstr_eq(" AAA", res);
  if (res) free(res);

#test insert_test_3
  char src[BUFFSIZE] = "AAA";
  char str[BUFFSIZE] = "b";
  char* res = s21_insert(src, str, 5);
  ck_assert_pstr_eq(s21_NULL, res);
  if (res) free(res);
