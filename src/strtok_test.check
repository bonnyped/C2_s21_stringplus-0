#suite strtok_test

#test strtok_test_1
  char a[] = "Hello, my friend";
  char b[] = "Hello, my friend";
  char *original, *my;
  original = strtok(a, ",");
  my = s21_strtok(b, ",");
  ck_assert_pstr_eq(original, my);
  original = strtok(NULL, "1");
  my = s21_strtok(NULL, "1");
  ck_assert_pstr_eq(original, my);
  original = strtok(NULL, "1");
  my = s21_strtok(NULL, "1");
  ck_assert_ptr_null(original);
  ck_assert_ptr_null(my);

#test strtok_test_2
  char a[] = ".../Hello../ /...Hello2...";
  char b[] = ".../Hello../ /...Hello2...";
  char *original, *my;
  original = strtok(a, "./ ");
  my = s21_strtok(b, "./ ");
  ck_assert_pstr_eq(original, my);
  original = strtok(NULL, "./ ");
  my = s21_strtok(NULL, "./ ");
  ck_assert_pstr_eq(original, my);
  ck_assert_pstr_eq(original, my);
  original = strtok(NULL, "./ ");
  my = s21_strtok(NULL, "./ ");
  ck_assert_pstr_eq(original, my);
  original = strtok(NULL, "./ ");
  my = s21_strtok(NULL, "./ ");
  ck_assert_ptr_null(original);
  ck_assert_ptr_null(my);
