#suite s21_sprintf_tests_p

#test test_1
char *a[200];
сhar res[200];
char resorig[200];
ck_assert_int_eq(s21_sprintf(res, "%p\n", a), sprintf(resorig, "%p\n", a));
ck_assert_ptr_eq(res, resorig);

#test test_2
char *a[200];
сhar res[200];
char resorig[200];
ck_assert_int_eq(s21_sprintf(res, "%%+ %p\n", a", a), sprintf(resorig, "%%+ %p\n", a", a));
ck_assert_ptr_eq(res, resorig);

